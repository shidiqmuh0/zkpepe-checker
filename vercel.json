{
    "version": 2,
    "routes": [
      {
        "src": "/(.*)",
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "OPTIONS, GET, POST, PUT, DELETE",
          "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept, Origin, Authorization"
        },
        "dest": "/$1"
      }
    ]
  }
  